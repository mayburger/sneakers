<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextFont">
        <attr name="typeface" />
    </declare-styleable>
    <declare-styleable name="EditTextFont">
        <attr name="typeface" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="regular" value="0" />
        <enum name="medium" value="1" />
        <enum name="bold" value="2" />
        <enum name="italic" value="3" />
        <enum name="black" value="4" />
        <enum name="uthman" value="5" />
    </attr>

</resources>